 journal = {Acta Paediatrica},
 keywords = {},
 location = {},
 number = {8},
 pages = {1230--1238},
 title = {Systematic review indicates postnatal growth in term infants born small‐for‐gestational‐age being associated with later neurocognitive and metabolic outcomes},
 volume = {106},
 year = {2017}
}

